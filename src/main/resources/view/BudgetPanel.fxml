<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>

<?import java.net.URL?>
<BorderPane fx:id="budgetPanel" minHeight="-Infinity" minWidth="-Infinity" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <stylesheets>
        <URL value="@DarkTheme.css" />
        <URL value="@Extensions.css" />
    </stylesheets>
   <top>
      <StackPane fx:id="budgetCardPlaceholder" prefHeight="150.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </top>
   <center>
      <StackPane fx:id="expenseListPanelPlaceholder" BorderPane.alignment="CENTER" />
   </center>
   <left>
      <Button fx:id="previous" mnemonicParsing="false" prefWidth="30.0" text="Button" BorderPane.alignment="CENTER" />
   </left>
   <right>
      <Button fx:id="next" mnemonicParsing="false" prefWidth="30.0" text="Button" BorderPane.alignment="CENTER" />
   </right>
</BorderPane>
